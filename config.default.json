{
	"$schema": "config.schema.json",
	"markets": [
		"US"
	],
	"fetchConsole": true,
	"fetchPC": true,
	"fetchEAPlay": true,
	"outputIndexing": "0-indexed",
	"treatEmptyStringsAsNull": true,
	"includedProperties": {
		"productTitle": true,
		"productId": true,
		"developerName": true,
		"publisherName": true,
		"productDescription": {
			"enabled": true,
			"preferShort": false
		},
		"images": {
			"enabled": true,
			"imageTypes": {
				"TitledHeroArt": -1,
				"SuperHeroArt": -1,
				"Logo": -1,
				"Poster": -1,
				"Screenshot": -1,
				"BoxArt": -1,
				"Hero": -1,
				"BrandedKeyArt": -1,
				"FeaturePromotionalSquareArt": -1
			}
		},
		"releaseDate": {
			"enabled": true,
			"format": "date"
		},
		"userRating": {
			"enabled": true,
			"aggregationInterval": "AllTime",
			"format": "percentage"
		},
		"pricing": {
			"enabled": true,
			"priceTypes": [
				"ListPrice",
				"MSRP",
				"WholesalePrice"
			],
			"missingPricePolicy": "useZero"
		},
		"categories": true
	}
}