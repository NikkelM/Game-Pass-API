{
	"$schema": "../../config.schema.json",
	"markets": [
		"US",
		"RU"
	],
	"language": "en-us",
	"platformsToFetch": [
		"console",
		"pc",
		"eaPlay"
	],
	"outputFormat": "productId",
	"treatEmptyStringsAsNull": true,
	"keepCompleteProperties": false,
	"includedProperties": {
		"productTitle": true,
		"productId": true,
		"developerName": true,
		"publisherName": true,
		"categories": true,
		"productDescription": {
			"enabled": true,
			"preferShort": false
		},
		"images": {
			"enabled": true,
			"imageTypes": {
				"TitledHeroArt": -1,
				"SuperHeroArt": -1,
				"Logo": -1,
				"Poster": -1,
				"Screenshot": -1,
				"BoxArt": -1,
				"Hero": -1,
				"BrandedKeyArt": -1,
				"FeaturePromotionalSquareArt": -1
			}
		},
		"releaseDate": {
			"enabled": true,
			"format": "dateTime"
		},
		"userRating": {
			"enabled": true,
			"aggregationInterval": "AllTime",
			"format": "stars"
		},
		"pricing": {
			"enabled": true,
			"priceTypes": [
				"ListPrice",
				"MSRP",
				"WholesalePrice"
			],
			"missingPricePolicy": "useNull"
		}
	}
}