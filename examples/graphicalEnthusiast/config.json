{
	"$schema": "../../config.schema.json",
	"markets": [
		"US"
	],
	"language": "en-us",
	"platformsToFetch": [
		"pc"
	],
	"outputFormat": "productTitle",
	"includedProperties": {
		"images": {
			"enabled": true,
			"imageTypes": {
				"TitledHeroArt": -1,
				"SuperHeroArt": -1,
				"Logo": -1,
				"Poster": -1,
				"Screenshot": -1,
				"BoxArt": -1,
				"Hero": -1,
				"BrandedKeyArt": -1,
				"FeaturePromotionalSquareArt": -1
			}
		},
		"userRating": {
			"enabled": true,
			"aggregationInterval": "AllTime",
			"format": "stars"
		}
	}
}