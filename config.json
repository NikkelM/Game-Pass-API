{
	"$schema": "config.schema.json",
	"market": "US",
	"fetchConsole": true,
	"fetchPC": true,
	"fetchEAPlay": true,
	"includedProperties": {
		"productTitle": true,
		"productId": true,
		"developerName": true,
		"publisherName": true,
		"productDescription": {
			"enabled": true,
			"preferShort": true,
			"extra": 1
		},
		"images": {
			"enabled": true,
			"maxImages": 10,
			"imageTypes": [
				"TitledHeroArt",
				"SuperHeroArt",
				"Logo",
				"Poster",
				"Screenshot",
				"BoxArt",
				"Hero",
				"BrandedKeyArt",
				"FeaturePromotionalSquareArt"
			],
			"extra": 1
		},
		"releaseDate": {
			"enabled": true,
			"format": "date",
			"extra": 1
		},
		"userRating": {
			"enabled": true,
			"aggregationInterval": "AllTime",
			"format": "percentage"
		},
		"extra": 1
	},
	"outputIndexing": "productTitle",
	"extra": 1
}