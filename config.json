{
	"$schema": "config.schema.json",
	"market": "US",
	"fetchConsole": true,
	"fetchPC": true,
	"fetchEAPlay": true,
	"includedProperties": {
		"productTitle": true,
		"productId": true,
		"developerName": true,
		"publisherName": true,
		"productDescription": {
			"enabled": true,
			"preferShort": true,
			"extra": 1
		},
		"images": {
			"enabled": true,
			"imageTypes": {
				"TitledHeroArt": 1,
				"extra": 1,
				"SuperHeroArt": 1,
				"Logo": 1,
				"Poster": 1,
				"Screenshot": 1,
				"BoxArt": 1,
				"Hero": 1,
				"BrandedKeyArt": 1,
				"FeaturePromotionalSquareArt": 1
			},
			"extra": 1
		},
		"releaseDate": {
			"enabled": true,
			"format": "date",
			"extra": 1
		},
		"userRating": {
			"enabled": true,
			"aggregationInterval": "AllTime",
			"format": "percentage",
			"extra": 1
		},
		"pricing":{
			"enabled": true,
			"priceTypes": [
				"ListPrice",
				"MSRP",
				"WholesalePrice",
				"extra",
				"MSRP"
			],
			"extra": 1
		},
		"categories": true,
		"extra": 1
	},
	"outputIndexing": "productTitle",
	"extra": 1
}